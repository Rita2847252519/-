   "source": [
    "a=input(\"请输入温度的符号:\")\n",
    "TempStr=eval(input(\"请输入温度的数值\"))\n",
    "if a in ['F','f']:\n",
    "    C=int((TempStr-32)/1.8)\n",
    "    print(\"转换后的温度是{:d}C\".format(C))\n",
    "elif a in ['C','c']:\n",
    "    F=int(1.8*TempStr+32)\n",
    "    print(\"转换后的温度是{:d}F\".format(F))\n",
    "else:\n",
    "    print(\"输入格式错误\")"
